<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.corti.medicapp.Main3Activity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="183dp"
        tools:layout_editor_absoluteY="183dp"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="131dp"
            tools:layout_editor_absoluteY="135dp">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ClaveUnica"
                tools:layout_editor_absoluteX="200dp"
                tools:layout_editor_absoluteY="259dp" />

            <EditText
                android:id="@+id/etclave"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="225dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="#006699"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="131dp"
            tools:layout_editor_absoluteY="135dp">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nombre"
                tools:layout_editor_absoluteX="200dp"
                tools:layout_editor_absoluteY="259dp" />

            <EditText
                android:id="@+id/etNombre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="225dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="137dp"
            tools:layout_editor_absoluteY="284dp">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Email"
                tools:layout_editor_absoluteX="121dp"
                tools:layout_editor_absoluteY="292dp" />

            <EditText
                android:id="@+id/etemail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="225dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:background="#006699"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="86dp"
            tools:layout_editor_absoluteY="286dp">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fecha de nacimiento"
                tools:layout_editor_absoluteX="121dp"
                tools:layout_editor_absoluteY="292dp" />

            <EditText
                android:id="@+id/etfecha"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="225dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="174dp"
            tools:layout_editor_absoluteY="329dp">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sexo"
                tools:layout_editor_absoluteX="121dp"
                tools:layout_editor_absoluteY="292dp" />

            <EditText
                android:id="@+id/etsexo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="225dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:background="#006699"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="210dp"
            tools:layout_editor_absoluteY="363dp">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tipo de sangre"
                tools:layout_editor_absoluteX="121dp"
                tools:layout_editor_absoluteY="292dp" />

            <EditText
                android:id="@+id/etsangre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="225dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="230dp"
            tools:layout_editor_absoluteY="412dp">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Telefono"
                tools:layout_editor_absoluteX="121dp"
                tools:layout_editor_absoluteY="292dp" />

            <EditText
                android:id="@+id/telefono"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="225dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#006699"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="147dp"
            tools:layout_editor_absoluteY="410dp">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Domicilio"
                tools:layout_editor_absoluteX="121dp"
                tools:layout_editor_absoluteY="292dp" />

            <EditText
                android:id="@+id/etdomicilio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="225dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="147dp"
            tools:layout_editor_absoluteY="410dp">

            <Button
                android:id="@+id/button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="alta"
                android:text="Registrar"
                tools:layout_editor_absoluteX="134dp"
                tools:layout_editor_absoluteY="283dp" />

            <Button
                android:layout_width="74dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="limpiar"
                android:text="Limpiar" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="consulta"
                android:text="Consultar" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="baja"
                android:text="Eliminar" />


        </LinearLayout>


    </LinearLayout>

</android.support.constraint.ConstraintLayout>
